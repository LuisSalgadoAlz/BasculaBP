model Vehiculo {
    id Int @id @default(autoincrement())
    placa String @unique
    modelo String?
    marca String?
    tipo String
    pesoMaximo Float?
    estado Boolean
    idEmpresa Int

    /* Relaciones con boleta */
    rlPlacasBoletas Boleta[]

    rEmpresaVehiculo Empresa @relation(fields: [idEmpresa], references: [id])
}

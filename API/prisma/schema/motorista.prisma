model Motoristas{
    id Int @id @default(autoincrement())
    nombre String
    telefono String?
    correo String? @unique
    estado Boolean
    idEmpresa Int

    /* Relacion con boleta */
    boletaMotoristas Boleta[]

    /* Relacion con vehiculo */
    rEmpresaM Empresa @relation(fields: [idEmpresa], references: [id])
}
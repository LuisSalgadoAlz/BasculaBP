model Socios{
    id Int @id @default(autoincrement())
    nombre String 
    tipo Int
    direccion String?
    telefono String?
    correo String? @unique

    /* Relacion con boleta */
    boletaSocios Boleta[]

    /* Relacion con direciones */
    drSocios Direcciones[]

    /* Relacion de Empresa */
    rEmpresa Empresa[]
}